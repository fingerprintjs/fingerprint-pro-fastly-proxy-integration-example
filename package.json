{
  "version": "0.1.1",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.4.1",
    "@fingerprintjs/eslint-config-dx-team": "^0.1.0",
    "@fingerprintjs/fingerprintjs-pro-server-api": "^5.0.0",
    "@fingerprintjs/prettier-config-dx-team": "^0.2.0",
    "@fingerprintjs/tsconfig-dx-team": "^0.0.2",
    "@noble/ciphers": "^1.0.0",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^29.5.12",
    "@types/pako": "^2.0.3",
    "esbuild": "^0.23.1",
    "fs": "0.0.1-security",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "pako": "^2.1.0",
    "path": "^0.12.7",
    "ts-loader": "^9.5.1",
    "typescript": "5.3.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@fastly/js-compute": "^3.21.4",
    "cookie": "0.5.0"
  },
  "scripts": {
    "prebuild": "tsc",
    "assemble": "js-compute-runtime build/index.js bin/main.wasm",
    "build": "pnpm run prebuild && pnpm run assemble",
    "deploy": "fastly compute publish",
    "dev": "fastly compute serve",
    "prepare": "husky",
    "lint": "eslint --ext .js,.ts,.mjs --ignore-path .gitignore --max-warnings 0 src/",
    "lint:fix": "pnpm lint --fix"
  },
  "lint-staged": {
    "*.{js,ts,mjs}": "pnpm lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
